{
  "QueueConfigurations": [
    {
      "Id": "cats-dogs-model-webhook",
      "Queue": "arn:minio:sqs::webhook:webhook",
      "Events": [
        "s3:ObjectCreated:Put"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "02_model_training/models/cats_and_dogs/"
            },
            {
              "Name": "suffix", 
              "Value": ".bin"
            }
          ]
        }
      }
    },
    {
      "Id": "cats-dogs-model-webhook-xml",
      "Queue": "arn:minio:sqs::webhook:webhook", 
      "Events": [
        "s3:ObjectCreated:Put"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "02_model_training/models/cats_and_dogs/"
            },
            {
              "Name": "suffix",
              "Value": ".xml" 
            }
          ]
        }
      }
    }
  ]
}